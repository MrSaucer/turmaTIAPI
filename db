
CREATE DATABASE projetoionic;
USE projetoionic;

CREATE TABLE users (
  ID INT AUTO_INCREMENT,
  name VARCHAR(255),
  user VARCHAR(255),
  password VARCHAR(255),
  PRIMARY KEY (ID)
);

